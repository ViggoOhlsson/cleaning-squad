<div class="booking">
	<h2 class="text1">Boka städning</h2>
	<h3 class="text2">Personuppgifter</h3>
	<form class="booking-form" action="" method="post">
		<input
			class="booking-input"
			type="text"
			name="address"
			placeholder="Address"
		/>
		<input
			class="booking-input"
			type="tel"
			name="zip"
			placeholder="Postnummer"
			inputmode="numeric"
			maxlength="5"
		/>
		<input class="booking-input" type="text" name="county" placeholder="Ort" />
		<input class="booking-input" type="date" name="date" placeholder="" />
		<input class="booking-input" type="time" name="time" />
		<h3 class="text2">Betalning</h3>
		<input
			class="booking-input"
			type="text"
			name="card-name"
			placeholder="Namn på kortinnehavaren"
		/>
		<input
			class="booking-input"
			type="tel"
			inputmode="numeric"
			pattern="[0-9\s]{13,19}"
			autocomplete="cc-number"
			maxlength="19"
			name="card-number"
			placeholder="Kortnummer"
		/>
		<input
			class="booking-input"
			type="month"
			id="start"
			name="start"
			min="2022-03"
			value="2022-04"
			name="card-date"
			placeholder="Utgångsdatum"
		/>
		<input
			class="booking-input"
			type="tel"
			inputmode="numeric"
			maxlength="3"
			placeholder="CVV"
			name="card-cvv"
		/>
		<h3 class="text2">Typ av städning</h3>
		<select
			class="booking-input"
			name="type"
			placeholder="Typ av städning"
		>
			<option value="Basic">Basic</option>
			<option value="Toppklass">Toppklass</option>
			<option value="Diamant">Diamant</option>
			<option value="Fönstertvätt">Fönstertvätt</option>
		</select>
		<input class="booking-btn" type="submit" value="Slutför bokning" />
	</form>
</div>